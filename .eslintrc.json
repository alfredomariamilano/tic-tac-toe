{
  "env": {
    "jest/globals": true,
    "browser": true
  },
  "extends": [
    "react",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jest",
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "no-use-before-define": "off"
  }
}